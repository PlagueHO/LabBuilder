           configfile="STANDALONE_INTERNET.DSC.ps1"
           logging="Y">
        <parameters>
          Scopes = @(
            @{ Name = 'Internet';
               Start = '131.107.0.50';
               End = '131.107.0.250';
               SubnetMask = '255.255.0.0';
               AddressFamily = 'IPv4'
            }
          )
          Reservations = @(
            @{ Name = 'SA-EDGE1';
               ScopeID = '131.107.0.0';
               ClientMACAddress = '000000000000';
               IPAddress = '131.107.0.50';
               AddressFamily = 'IPv4'
            },
            @{ Name = 'SA-EDGE2';
               ScopeID = '131.107.0.0';
               ClientMACAddress = '000000000001';
               IPAddress = '131.107.0.51';
               AddressFamily = 'IPv4'
