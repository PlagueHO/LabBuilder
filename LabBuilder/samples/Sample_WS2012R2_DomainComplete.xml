                subnetmask="8"
                dnsserver="131.107.0.1"/>
          <ipv6 address="2001:54b9:f782:ae41::10"
                defaultgateway=""
                subnetmask="64"
                dnsserver="2001:54b9:f782:ae41::10"/>
        </adapter>
