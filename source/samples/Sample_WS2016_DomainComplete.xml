              @{ Name = 'SA-SUBCA';
                 ScopeID = '192.168.128.0';
                 ClientMACAddress = '000000000009';
                 IPAddress = '192.168.128.23';
                 AddressFamily = 'IPv4'
              },
              @{ Name = 'SA-FS1';
                 ScopeID = '192.168.128.0';
                 ClientMACAddress = '00000000000A';
                 IPAddress = '192.168.128.24';
                 AddressFamily = 'IPv4'
              },
              @{ Name = 'SA-FS2';
                 ScopeID = '192.168.128.0';
                 ClientMACAddress = '00000000000B';
                 IPAddress = '192.168.128.25';
                 AddressFamily = 'IPv4'
              }
            )
          ScopeOptions = @(
              @{ ScopeID = '192.168.128.0';
