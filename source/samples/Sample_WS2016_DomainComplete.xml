    <vm name="SA-WDS"
      template="Template Windows Server 2016 Datacenter Full"
      computername="SA-WDS">
      <datavhds>
        <datavhd vhd="LABBUILDER.COM SA-WDS Data Disk.vhdx" size="10GB" type="dynamic" />
